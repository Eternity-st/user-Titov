<Window x:Class="Demoekz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demoekz"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="53"/>
            <RowDefinition Height="500"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Background="AliceBlue" Orientation="Horizontal" >
            <Button x:Name="BackMain" Width="50" Height="20" Margin="700 0 0 0" Click="BackMain_Click" />

        </StackPanel>
        <Frame x:Name="BtnMain" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
